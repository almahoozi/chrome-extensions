{
  "manifest_version": 3,
  "name": "GitHub PR Whitespace Ignorer",
  "version": "1.0",
  "permissions": ["webNavigation", "tabs", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "host_permissions": ["https://github.com/*"]
}
